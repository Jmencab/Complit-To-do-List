@import 'nib'

html 
  box-sizing border-box;

 
*, *:before, *:after {
  box-sizing: inherit;
}
 
 
body 
  font-family: sans-serif;
  font-size: 16px;
  background: #ccc;


 
.container {
  max-width: 900px;
  margin: o auto;
  margin-bottom: 50px;
  min-height: 100%;
  background: white;
  box-shadow: 0 2px 10px 0 rgba(0, 0, 0, 0.16), 0 2px 5px 0 rgba(0, 0, 0, 0.26);
}

.container img {
 margin-left: 11em;
}

 
header {
  padding: 20px 15px 15px 15px;
  position: relative;

}
 
h1 {
  font-size: 2em;
  margin: 0;
  margin-bottom: 10px;
  display: inline-block;
  margin-right: 1em;
  text-align: center;
}
 
.new-resolution input[type="text"] {
  padding: 10px 0;
  background: transparent;
  border: none;
  width: 50%;
  padding-right: 80px;
  font-size: 1em;
  border-bottom: 1px solid rgba(0,0,0,0.3);
}
 
.new-resolution input:focus{
  outline: 0;
}
 
ul {
  margin: 0;
  padding: 0;
  background: white;
}
 
.delete {
  float: right;
  font-weight: bold;
  background: #DE4F4F;
  color: #FFF;
  font-size: 1em;
  padding: 5px 10px;
  border: none;
  border-radius: 4px;;
}
 
li {
  position: relative;
  list-style: none;
  padding: 20px 15px;
  border-bottom: #eee solid 1px;
}
 
li .text {
  margin-left: 10px;
}
 
li.checked {
  color: #888;
}
 
li.checked .text {
  text-decoration: line-through;
}

/*The Button for homepage*/
div.try {
  height: 50px;
  width: 120px;
  border-color: #6495ED;
  background-color: #BCD2EE;
  border-style: solid;
  border-width: 2px;
  border-radius: 5px;
  margin: auto;
  text-align: center;
}

div.navbar {
background-color: #333;

}

div.navbar ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: red; 
}

div.navbar ul li {
  float: left;
}

div.navbar ul li a {
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
}

/* Change the link color to #111 (black) on hover */
div.navbar ul li a:hover {
    background-color: #111;
}

div.heads {
  background-color: #333;
  display: block;
}

div.heads img {
  margin-left: 30px;
  margin-right: 50px;
}

div.footers img {
  margin-top: 30px;
  margin-left: 30px;
}

div.footers p {
  margin-left: 30px;
}


div.UsefulContainer ul li ul li {
   list-style-type: circle;
   margin-left: 40px;
 
}
